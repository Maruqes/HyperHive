MODE=dev # dev or prod
QEMU_UID=107
QEMU_GID=107
SPRITE_MIN=9600
SPRITE_MAX=9700
MASTER_IP=192.168.1.69

MAIN_LINK=http://127.0.0.1:9595   #WITHOUT THE "/" in the end

# Optional comma separated list of panels to enable. Leave both vars empty to enable every panel.
# Panels: VISITORS, REQUESTS, REQUESTS_STATIC, NOT_FOUND, HOSTS, OS, BROWSERS, VISIT_TIMES,
# VIRTUAL_HOSTS, REFERRERS, REFERRING_SITES, KEYPHRASES, STATUS_CODES, REMOTE_USER, CACHE_STATUS,
# GEO_LOCATION, MIME_TYPE, TLS_TYPE
GOACCESS_ENABLE_PANELS=
# Optional comma separated list of panels to hide (same panel names as above)
GOACCESS_DISABLE_PANELS=

# Optional override; by default drop your .mmdb file under ./geoipdb/
GOACCESS_GEOIP_DB=
