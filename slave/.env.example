MASTER_IP=127.0.0.1          # IP or resolvable hostname of the master
SLAVE_IP=127.0.0.1           # IP or hostname representing this slave
#THE SLAVE ON MASTER SERVER NEEDS TO HAVE SAME SLAVE_IP AND MASTER_IP

OTHER_SLAVE1_IP=192.168.1.125 # Optional, can add OTHER_SLAVE2_IP, OTHER_SLAVE10_IP, OTHER_SLAVE25_IP 
OTHER_SLAVE2_IP=192.168.1.126
OTHER_SLAVE3_IP=192.168.1.127

MODE=dev # dev or prod
MACHINE_NAME=slave1
VNC_MIN_PORT=12000
VNC_MAX_PORT=12999

DIRTY_RATIO_PERCENT=35            # % of RAM allowed to stay dirty before writers are throttled; set to 0 to revert back to the built-in default (15%).
DIRTY_BACKGROUND_RATIO_PERCENT=10 # % of RAM at which the kernel begins background flushing; set to 0 to derive from DIRTY_RATIO_PERCENT (~25%) or revert to the auto-derived value when DIRTY_RATIO_PERCENT is 0.

# Keep DIRTY_BYTES and DIRTY_BACKGROUND_BYTES at 0 (unset) so the percentage knobs remain authoritative.

QEMU_UID=107
QEMU_GID=107

K3S_VERSION=stable            # Optional: set to an explicit k3s version tag (e.g. v1.28.7+k3s1); leave empty for the installer default.
EXTRA_K8S_IPS=192.168.1.10,192.168.1.11 # Optional: comma-separated extra TLS SAN IPs for k3s (in addition to SLAVE_IP).

